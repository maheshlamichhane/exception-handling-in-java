package handling.checked.exception.with_throws.for_parent.handling;

public class Test {

	public static void main(String[] args) {
		
		doStuff();

	}

	private static void doStuff() {
		doMoreStuff();
		
	}

	private static void doMoreStuff() throws InterruptedException {
		Thread.sleep(5000);
		
	}

}
